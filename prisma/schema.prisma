generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Card {
  id        String   @id
  name      String
  number    String
  set       String
  image     String
  rarity    String
  spotlight Float
  market    Float
  cachedAt  DateTime @default(now())

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
